# Environment Variables Example
# Copy this file to .env and update the values as needed

# API Configuration
REACT_APP_API_URL=https://api.catchycompany.sa
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_MAINTENANCE_MODE=false

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_FALLBACK_LANGUAGE=ar

# Contact Information
REACT_APP_CONTACT_EMAIL=info@catchycompany.sa
REACT_APP_CONTACT_PHONE=+966XXXXXXXXX

# Social Media
REACT_APP_TWITTER_HANDLE=catchycompany
REACT_APP_FACEBOOK_PAGE=catchycompany
REACT_APP_INSTAGRAM_HANDLE=catchycompany

# Google Maps API (if applicable)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Note: Never commit actual API keys or sensitive information to version control.
# This file is for documentation purposes only.
